# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar Address

scalar Long

input SendInput {
    toAddress: Address!
}

type TxResponse {
    hash: String!
    code: Int!
    rawLog: String
    gasWanted: Long!
    gasUsed: Long!
}

type Mutation {
    send(input: SendInput!): TxResponse!
}

type Query {
    health: String
}
